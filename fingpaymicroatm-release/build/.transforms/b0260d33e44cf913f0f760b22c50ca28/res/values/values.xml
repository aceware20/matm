    <style name="CustomAlertDialog" parent="@android:style/Theme.Panel">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@android:color/transparent</item>
        <item name="android:bottomDark">@android:color/transparent</item>
        <item name="android:bottomMedium">@android:color/transparent</item>
        <item name="android:centerBright">@android:color/transparent</item>
        <item name="android:centerDark">@android:color/transparent</item>
        <item name="android:centerMedium">@android:color/transparent</item>
        <item name="android:fullBright">@drawable/dialog</item>
        <item name="android:fullDark">@drawable/dialog</item>
        <item name="android:topBright">@android:color/transparent</item>
        <item name="android:topDark">@android:color/transparent</item>
    </style>
    <style name="StyledDialog" parent="@android:style/Theme.Panel">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/btn_bg</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="header_color_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">@color/white</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/header_color</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="heading_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">54dp</item>
        <item name="android:shadowColor">#000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="mainlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/app_bg</item>
    </style>
    <style name="text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">@color/white</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/black</item>
